{
  "name": "drupal/search_api_postgresql",
  "type": "drupal-module",
  "description": "PostgreSQL backend for Search API using native full-text search capabilities with AI text embeddings support and secure credential storage",
  "keywords": ["drupal", "search", "postgresql", "full-text", "ai", "embeddings", "vector", "security"],
  "license": "GPL-2.0+",
  "homepage": "https://www.drupal.org/project/search_api_postgresql",
  "minimum-stability": "dev",
  "support": {
    "issues": "https://www.drupal.org/project/issues/search_api_postgresql",
    "source": "https://git.drupalcode.org/project/search_api_postgresql"
  },
  "require": {
    "drupal/core": "^10.4 || ^11",
    "drupal/search_api": "^1.31",
    "drupal/key": "^1.16",
    "ext-pdo_pgsql": "*",
    "guzzlehttp/guzzle": "^7.0"
  },
  "require-dev": {
    "drupal/core-dev": "^10.4 || ^11"
  },
  "suggest": {
    "drupal/facets": "For faceted search functionality",
    "drupal/search_api_autocomplete": "For autocomplete functionality",
    "pgvector/pgvector": "PostgreSQL extension for vector similarity search (required for AI embeddings)"
  },
  "extra": {
    "drush": {
      "services": {
        "drush.services.yml": "^10 || ^11"
      }
    }
  }
}